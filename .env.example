# Real Estate API Configuration
# Copy this file to .env and fill in your actual API keys

# Apify API Configuration
APIFY_API_TOKEN=your_apify_api_token_here
APIFY_REAL_ESTATE_ACTOR_ID=petr_cermak/real-estate-scraper

# Zillow API Configuration (if available)
ZILLOW_API_KEY=your_zillow_api_key_here
ZILLOW_API_HOST=zillow-com1.p.rapidapi.com

# RentBerry API Configuration
RENTBERRY_API_KEY=your_rentberry_api_key_here
RENTBERRY_API_URL=https://api.rentberry.com/v1

# Rentals.com API Configuration
RENTALS_API_KEY=your_rentals_api_key_here
RENTALS_API_URL=https://api.rentals.com/v1

# RapidAPI Configuration (for various real estate APIs)
RAPIDAPI_KEY=your_rapidapi_key_here
RAPIDAPI_HOST=realty-in-us.p.rapidapi.com

# Application Configuration
NODE_ENV=development
API_TIMEOUT=30000
MAX_RESULTS_PER_API=50
USE_MOCK_DATA=true
